record( ai, "${P}${R}M${MODULE_ID}:C${CHANNEL_ID}:VoltageMeasure" ) {
  field( DTYP, "isegHAL" )
  field( INP,  "@${CAN_LINE}.${MODULE_ID}.${CHANNEL_ID}.VoltageMeasure can0" )
  field( SCAN, "${SCAN=.5 second}" )
  field( TSE,  "-2" )
  field( FLNK, "${P}${R}M${MODULE_ID}:C${CHANNEL_ID}:VoltageMeasure:Counter" )
}
record( calc,  "${P}${R}M${MODULE_ID}:C${CHANNEL_ID}:VoltageMeasure:Counter" )
{
  field( CALC, "VAL+1" )
}
record( calc, "${P}${R}M${MODULE_ID}:C${CHANNEL_ID}:VoltageMeasure:Rate" )
{
	field(INPA, "0" ) # Initial counter value
	field(INPB, "${P}${R}M${MODULE_ID}:C${CHANNEL_ID}:VoltageMeasure:Counter NPP NMS" )
	field(INPC, "1.0")                                # Delta time in seconds
	field(INPE, "$(RATE_SMOOTH=0.0)")                 # Smoothing factor
	field(CALC, "(VAL*E)/C+MAX(0,B-A)*(1-E)/C;A:=B")
	field(PREC, "2" )
	field(EGU,  "Hz" )
	field(SCAN, "1 second")
	field(LOW,  "0")
	field(LSV,  "MINOR")
	field(ADEL, "0.1")
	info(autosaveFields, "ADEL DESC LOLO LOW HIGH HIHI LLSV LSV HSV HHSV MDEL PREC C E SCAN EGU")
}

