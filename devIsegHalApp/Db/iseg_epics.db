record( mbbiDirect, "${P}${R}Crate:EventStatusLow" ) {
  field( DTYP, "isegHAL" )
  field( INP,  "@${CAN_LINE}.${DEVICE_ID}.EventStatus can0" )
  field( NOBT, "16" )
  field( SHFT, "0" )
  field( SCAN, "I/O Intr" )
  field( TSE,  "-2" )
  field( FLNK, "${P}${R}Crate:EventStatusLow:Counter" )
}
record( calc, "${P}${R}Crate:EventStatusLow:Counter" )
{
  field( CALC, "VAL+1" )
}
record( calc, "${P}${R}Crate:EventStatusLow:Rate" )
{
	field(INPA, "0" ) # Initial counter value
	field(INPB, "${P}${R}Crate:EventStatusLow:Counter NPP NMS" )
	field(INPC, "1.0")                                # Delta time in seconds
	field(INPE, "$(RATE_SMOOTH=0.0)")                 # Smoothing factor
	field(CALC, "(VAL*E)/C+MAX(0,B-A)*(1-E)/C;A:=B")
	field(PREC, "2" )
	field(EGU,  "Hz" )
	field(SCAN, "1 second")
	field(LOW,  "0")
	field(LSV,  "MINOR")
	field(ADEL, "0.1")
	info(autosaveFields, "ADEL DESC LOLO LOW HIGH HIHI LLSV LSV HSV HHSV MDEL PREC C E SCAN EGU")
}


